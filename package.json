{
  "name": "badging-t3",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "compilerOptions": {
    "esModuleInterop": true
  },
  "scripts": {
    "style-dictionary": "style-dictionary build --config ./style-dictionary/config.json",
    "prebuild": "npm run style-dictionary",
    "postinstall": "prisma generate",
    "build": "next build",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:push:seed": "pnpm db:push && pnpm db:seed",
    "db:push:reset": "pnpm db:push --force-reset",
    "db:studio": "prisma studio -n 0.0.0.0 -p 3100",
    "lint": "next lint",
    "start": "next start",
    "dev": "concurrently npm:next:dev npm:db:studio",
    "next:dev": "next dev"
  },
